const events = [
    {
        name: 'Raid',
        description: 'A group of nomads has raided your territory',
        effect: player => {
            player.soldiers = Math.max(0, player.soldiers - 5);
            return player;
        },
    },
    {
        name: 'Royal Gift',
        description: 'Another ruler has given you some treasure',
        effect: player => {
            player.gold += 5;
            return player;
        }
    },
];

